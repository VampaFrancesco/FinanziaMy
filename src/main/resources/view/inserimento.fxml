<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorInserimento" prefHeight="460.0" prefWidth="330.0" styleClass="anchorPage" stylesheets="@grafica.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.cdvd.controller.InserimentoController">
   <children>
      <Label fx:id="importo" layoutX="147.0" layoutY="84.0" prefHeight="30.0" prefWidth="46.0" text="Importo" />
      <Label fx:id="text" layoutX="58.0" layoutY="14.0" prefHeight="59.0" prefWidth="213.0" text="Inserimento Transazione" textFill="#bbe944" wrapText="true">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <contextMenu>
            <ContextMenu>
              <items>
                <MenuItem mnemonicParsing="false" text="Unspecified Action" />
              </items>
            </ContextMenu>
         </contextMenu>
      </Label>

      <TextField fx:id="inserisciImporto" layoutX="82.0" layoutY="115.0" prefHeight="25.0" prefWidth="174.0" promptText="inserisci importo" />
      <TextField fx:id="causale" layoutX="83.0" layoutY="193.0" prefHeight="25.0" prefWidth="174.0" promptText="causale" />
      <ComboBox fx:id="categoriaList" layoutX="83.0" layoutY="239.0" prefHeight="26.0" prefWidth="174.0" promptText="Categoria" styleClass="button" stylesheets="@grafica.css" />
      <DatePicker fx:id="data" layoutX="83.0" layoutY="295.0" prefHeight="26.0" prefWidth="174.0" />
      <Button fx:id="inserisci" layoutX="82.0" layoutY="369.0" mnemonicParsing="false" onAction="#inserisciTransazione" prefHeight="26.0" prefWidth="174.0" styleClass="button" stylesheets="@grafica.css" text="Inserisci" />
      <Label fx:id="labelCausale" layoutX="147.0" layoutY="162.0" prefHeight="30.0" prefWidth="46.0" text="Causale" />
      <Button fx:id="annulla" layoutX="82.0" layoutY="404.0" mnemonicParsing="false" onAction="#annulla" prefHeight="26.0" prefWidth="174.0" styleClass="button" stylesheets="@grafica.css" text="Annulla" />

   </children>
</AnchorPane>
